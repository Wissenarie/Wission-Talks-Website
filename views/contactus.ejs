<%- include('partials/header'); -%>
    <style>
        .banner {
            width: 100%;
            height: 400px;
            background-image: url("../public/images/speakersBG.jpg");
            background-color: #cccccc;
            display: flex;
            justify-content: center;
        }
        
        .map-card {
            border: 5px solid white;
        }
        
        .map-holder {
            border-radius: 15px;
        }
        
        .icon-bg {
            background-color: blue;
            border-radius: 40%;
            padding: 10px;
            color: white;
            width: 30px;
            text-align: center;
        }
        
        .contact-text {
            font-weight: 600;
        }
        
        .login-btn {
            border-radius: 15px;
            font-weight: 600;
            float: center;
        }
        
        .btn-div {
            display: flex;
            justify-content: center;
        }
        
        h1 {
            margin: 250px auto;
            color: white;
            font-weight: 700;
            letter-spacing: 2px;
            text-decoration: underline;
        }
    </style>

    <!-- Navbar section  -->
    <div class="banner">
        <%- include('partials/navbar'); -%>
            <h1><span class="contact">Contact </span><span>Us</span></h1>
    </div>
    <!-- end of navbar section  -->

    <!-- Start of contact form  -->
