<%-include("partials/header")-%>
    <style>
        .banner {
            width: 100%;
            height: 400px;
            background-image: url("../public/images/speakersBG.jpg");
            background-color: #cccccc;
            display: flex;
            justify-content: center;
        }
        
        h1 {
            margin: 250px auto;
            color: white;
            font-weight: 700;
            letter-spacing: 2px;
            text-decoration: underline;
        }
        
        .circle {
            height: 60px;
            width: 60px;
            background-color: #22d1ee;
            border-radius: 50%;
            color: #ffffff;
            text-align: center;
        }
        
        small {
            font-weight: 400;
            font-size: small;
        }
        
        .circle p {
            margin-top: -6px;
            font-weight: 600;
            font-size: large;
        }
        
        .timeline-holder {
            display: flex;
            justify-content: space-evenly;
        }
        
        .timeline-div {
            height: 50px;
            border-radius: 15px;
            margin-top: 6px;
            border: none;
            font-family: Verdana, Geneva, Tahoma, sans-serif;
            font-size: large;
            color: black;
        }
    </style>

    <div class="banner">
        <%- include('partials/navbar'); -%>
            <h1><span class="contact">TIMELINE</span></h1>
    </div>

    <div class="container p-5">

        <% data.forEach((timeline)=>{ %>
            <div class="timeline-holder">
                <div class="circle mb-5 py-3">
                    <small>Dec</small>
                    <p>15</p>
                </div>
                <div class="col-md-7 timeline-div card shadow-lg py-2">
                    <%=timeline.Name%>
                </div>
            </div>
            <% })%>

